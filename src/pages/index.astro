---
import Layout from '../layouts/Layout.astro';
import Page from '../components/index'
import { getData } from './getData'
import { GetImageUrl } from '../utils/getUrl';
const { sections } = await getData()
---
<Layout>
	{sections?.map((section) => {
		section = {...section , GetImageUrl}

		switch (section.__typename) {
			case "ComponentSectionNavbar":
				return <Page.Navbar {...section} />
			case "ComponentSectionSlide":
				return <Page.Slider {...section} />
			case "ComponentSectionCards":
				return <Page.Values {...section} />
			case "ComponentSectionAbout":
				return <Page.About  {...section} />
			case "ComponentSectionAreas":
				return <Page.Areas {...section} />
			case "ComponentSectionServices":
				return <Page.Services {...section} />
			case "ComponentSectionPartners":
				return <Page.Partners {...section} />
			case "ComponentSectionContact":
				return <Page.Contact {...section} />
			default:
				return <Page.Testimonials {...section} />
	}})}
</Layout>
